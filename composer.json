{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "frameworkdesign/laravel-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4.0",
        "inertiajs/inertia-laravel": "^2.0.10",
        "laravel/fortify": "^1.32.0",
        "laravel/framework": "^12.39.0",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.10.1",
        "laravel/wayfinder": "^0.1.12",
        "nunomaduro/essentials": "^1.0.1",
        "spatie/laravel-data": "^4.18",
        "spatie/laravel-permission": "^6.23",
        "spatie/laravel-ray": "^1.42.0",
        "spatie/laravel-typescript-transformer": "^2.5.2",
        "spatie/typescript-transformer": "^2.5",
        "staudenmeir/belongs-to-through": "^2.17"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16.1",
        "barryvdh/laravel-ide-helper": "^3.6",
        "driftingly/rector-laravel": "^2.1.3",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.8.0",
        "laravel/boost": "^1.8.2",
        "laravel/pail": "^1.2.3",
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.3",
        "peckphp/peck": "^0.2.0",
        "pestphp/pest": "^4.1.4",
        "pestphp/pest-plugin-browser": "^4.1.1",
        "pestphp/pest-plugin-laravel": "^4.0.0",
        "pestphp/pest-plugin-type-coverage": "^4.0.3",
        "rector/rector": "^2.2.8",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "pnpm install",
            "pnpm run build"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan clear",
            "@update:requirements"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "update:requirements": [
            "composer bump"
        ],
        "lint": [
            "rector",
            "pint --parallel",
            "npm run lint"
        ],
        "test:typos": "peck",
        "test:type-coverage": "pest --type-coverage --min=100",
        "test:lint": [
            "pint --parallel --test",
            "rector --dry-run",
            "npm run test:lint"
        ],
        "test:unit": "pest --parallel --coverage",
        "test:vitest": "npm run test:unit",
        "test:types": [
            "phpstan --memory-limit=2G",
            "npm run test:types"
        ],
        "test": [
            "@test:typos",
            "@test:type-coverage",
            "@test:lint",
            "@test:unit",
            "@test:vitest",
            "@test:types"
        ],
        "transform-types": [
            "@php artisan typescript:transform --output ts/types/generated.d.ts"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
